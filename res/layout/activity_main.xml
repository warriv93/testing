<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res/com.ThailandCrew.skalman2"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/bakgrund"
    android:orientation="horizontal"
    tools:context="com.ThailandCrew.skalman2.MainActivity"
    tools:ignore="MergeRootFrame" >

    <ImageButton
        android:id="@+id/kalenderbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginBottom="40dp"
        android:layout_marginLeft="40dp"
        android:src="@drawable/knappkalender" />

    <ImageButton
        android:id="@+id/helpbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginBottom="40dp"
        android:layout_marginLeft="90dp"
        android:src="@drawable/knapphjalp" />

</LinearLayout>